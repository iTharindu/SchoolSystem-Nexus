    link(rel='stylesheet prefetch', href='/fonts/font-awesome-4.7.0/css/font-awesome.css')
    link(rel='stylesheet prefetch', href='/fonts/font-awesome-4.7.0/css/icon-font.min.css')
    link(rel='stylesheet', href= '/css/table_assignment.css')
    link(rel='stylesheet', href= '/css/logout.css')
    link(rel='stylesheet', href= '/css/modal.css')
    link(rel='stylesheet', href= '/css/main.css')
